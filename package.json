{
  "name": "reference",
  "private": true,
  "version": "0.0.0",
  "main": "src/electron/main.js",
  "description": "A simple text editor",
  "author": {
    "name": "Magne Tenstad"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build --config vite.electron.config.ts",
